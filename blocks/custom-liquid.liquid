<div class="custom-logo__container">
  {{ block.settings.custom_liquid }}
</div>

{% stylesheet %}
  .custom-logo__container svg {
    height: fit-content;
  }
{% endstylesheet %}

{% schema %}
{
  "name": "t:names.custom_liquid",
  "tag": null,
  "settings": [
    {
      "type": "liquid",
      "id": "custom_liquid",
